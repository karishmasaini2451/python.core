   "source": [
    "def sum_list(n):\n",
    "    sum_numbers=0\n",
    "    for  i in n:\n",
    "        sum_numbers=sum_numbers+i\n",
    "    print(sum_numbers)\n",
    "    \n",
    "sum_list([1,2,3,4])"
   ]
